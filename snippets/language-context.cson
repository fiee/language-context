".text.tex.context":
  "\\about":
    comment: "Provides the complete title of a referenced block of text"
    prefix: "about"
    body: "\\\\about" +
      "{${1:name}}" +
      "[${2:ref}]" +
      "$3"
  "\\adaptlayout":
    comment: "Adapt the layout of one page to fit more or less lines in it"
    prefix: "adaptlayout"
    body: "\\\\adaptlayout" +
      "[${2:page number(s)}]" +
      "[${3:height=name,lines number=+1, -1}]" +
      "$4"
  "\\arg":
    omment: "Surround given text with braces"
    prefix: "arg"
    body: "\\\\arg" +
      "{${1:name}}" +
      "$2"
  "\\at":
    prefix: "at"
    body: "\\\\at" +
      "${1:{${2:prefix}\\}}" +
      "${3:{${4:suffix}\\}}" +
      "[${6:ref}]" +
      "$7"
  "\\atpage":
    comment: "Reference label page"
    prefix: "atpage"
    body: "\\\\atpage" +
      "[${1:ref}]" +
      "$2"
  "\\blank":
    comment: "Insert vertical blank space"
    prefix: "blank"
    body: "\\\\blank" +
      "[${1:big}]" +
      "$2"
  "\\cap":
    comment: "Small caps text inside"
    prefix: "cap"
    body: "\\\\cap" +
      "{${1:name}}" +
      "$2"
  "\\chapter":
    prefix: "chapter"
    body: "\\\\chapter" +
      "${1:[${2:refs for this chapter}]}" +
      "{${3:title}}" +
      "$4"
  "\\clip":
    comments: '''
      Crop image piece at position (x, y).
      Number of pieces is specified by \\setupclipping.
    '''
    prefix: "clip"
    body: "\\\\clip" +
      "[x=${1:name}, y=${2:name}]" +
      "{${3:figure or text}}" +
      "$4"
  "\\color":
    prefix: "color"
    body: "\\\\color" +
      "[${1:color}]" +
      "{${2:text to color}}" +
      "$3"
  "\\column":
    comment: "Split column inside \\start/stopcolumns"
    prefix: "column"
    body: "\\\\column\n" +
      "$0"
  "\\convertnumber":
    prefix: "convertnumber"
    body: "\\\\convertnumber" +
      "{${1:mode}}" +
      "{${2:number}}" +
      "$3"
  "\\crlf":
    comments: "Carriage return command"
    prefix: "crlf"
    body: "\\\\crlf" +
      "$1"
  "\\currentdate":
    prefix: "currentdate"
    body: "\\\\currentdate" +
      "[${1:weekday,month,day,\\{, \\},year}]" +
      "$2"
  "\\date":
    prefix: "date"
    body: "\\\\date" +
      "[d=${1:name},m=${2:name},y=${3:name}}]" +
      "[${4:weekday,month,day,\\{,\\},year}]" +
      "$5"
  "\\define":
    prefix: "define"
    body: "\\\\define" +
      "${1:[${2:number of arguments}]}" +
      "\\\\${3:command}" +
      "{${4:Example #1}}" +
      "$5"
  "\\defineblank":
    prefix: "defineblank"
    body: "\\\\defineblank" +
      "[${1:measure name}]" +
      "[${2:dimension}]" +
      "$3"
  "\\defineblock":
    comment: "Define a reusable text format (ex. questions)"
    prefix: "defineblock"
    body: "\\\\defineblock" +
      "[${1:name of the block}]" +
      "$2"
  "\\definebodyfont":
    prefix: "definebodyfont"
    body: "\\\\definebodyfont" +
      "[${1:5pt name 12pt default}]" +
      "[${2:rm ss tt mm hw cg}]" +
      "[${3:options}]" +
      "$4"
  "\\definebodyfontenvironment":
    comment: '''Set up font scaling for "[tf]a-[tf]"d'''
    prefix: "definebodyfontenvironment"
    body: "\\\\definebodyfontenvironment" +
      "[${1:font size}]" +
      "[${a=2pt, d=4pt}]" +
      "$3"
  "\\definecolor":
    prefix: "definecolor"
    body: "\\\\definecolor" +
      "[${1:color name}]" +
      "[${2:h=FF0000}]" +
      "$3"
  "\\definecolorgroup":
    prefix: "definecolorgroup"
    body: "\\\\definecolorgroup" +
      "[${1:color set name}]" +
      "${2:[${3:rgb cmyk gray s}]}" +
      "[${1.00:0.85:0.80, name}]" +
      "$5"
  "\\definecombinedlist":
    comment: "Merge lists created by \\definelist"
    prefix: "definecombinedlist"
    body: "\\\\definecombinedlist" +
      "[${1:name}]" +
      "[${2:chapter,section}]" +
      "${3:[${4:level=subsection}]}" +
      "$5"
  "\\defineconversion":
    prefix: "defineconversion"
    body: "\\\\defineconversion" +
      "[${1:conversion name}]" +
      "[${2:list of strings to convert numbers into}]" +
      "$3"
  "\\definedescription":
    comment: "Define a new description with an associated style"
    prefix: "definedescription"
    body: "\\\\definedescription" +
      "[${1:name}]" +
      "${2:[${3: \\\\setupdescriptions parameters}]}" +
      "$4"
  "\\defineenumeration":
    comment: "Define the commands for a numbered text element"
    prefix: "defineenumeration"
    body: "\\\\defineenumeration" +
      "[${1:name}]" +
      "${4:[${5:\\\\setupenumerations parameters}]}" +
      "$6"
  "\\definefiguresymbol":
    comment: "Define a symbol using a figure"
    prefix: "definefiguresymbol"
    body: "\\\\definefiguresymbol" +
      "[${1:name}]" +
      "[${2:text}]" +
      "${3:[${4:\\\\externalfigure parameters}]}" +
      "$5"
  "\\definefloat":
    prefix: "definefloat"
    body: "\\\\definefloat" +
      "[${1:singular name}]" +
      "[${2:plural name}]" +
      "${3:[${4:parent}]}" +
      "$5"
  "\\definefont":
    prefix: "definefont"
    body: "\\\\definefont" +
      "[${1:name}]" +
      "[${2:file}]" +
      "$3"
  "\\defineframed":
    prefix: "defineframed"
    body: "\\\\defineframed" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setupframedtexts parameters}]}" +
      "$4"
  "\\defineframedtext":
    prefix: "defineframedtext"
    body: "\\\\defineframedtext" +
      "[${1:name}]" +
      "${2:[${3:see \\\\setupframedtexts parameters}]}" +
      "$4"
  "\\definehead":
    prefix: "definehead"
    body: "\\\\definehead" +
      "[${1:name}]" +
      "[${2:parent head style}]" +
      "${3:[${4:see \\\\setuphead parameters}]}" +
      "$5"
  "\\defineindenting":
    prefix: "defineindenting"
    body: "\\\\defineindenting" +
      "[${1:name}]" +
      "[${2:see \\setupindentations parameters}]" +
      "$3"
  "\\definelabel":
    comment: "Define an autonumbered label type, including a command to place one"
    prefix: "definelabel"
    body: "\\\\definelabel" +
      "[${1:name}]" +
      "[${2:text=${3:name}, location=${4:name}$5}]" +
      "$6"
  "\\definelist":
    prefix: "definelist"
    body: "\\\\definelist" +
      "[${1:name}]" +
      "${4:[see \\setuplist parameters]}" +
      "$6"
  "\\definemakeup":
    prefix: "definemakeup"
    body: "\\\\definemakeup" +
      "[${1:name}]" +
      "[${2:see \\setupmakeup parameters}]" +
      "$3"
  "\\definemarking":
    prefix: "definemarking"
    body: "\\\\definemarking" +
      "[${1:name}]" +
      "${2:[${3:name}]}" +
      "$4"
  "\\defineoverlay":
    prefix: "defineoverlay"
    body: "\\\\defineoverlay" +
      "[${1:name}]" +
      "[${2:text}]" +
      "$3"
  "\\definepalet":
    prefix: "definepalet"
    body: "\\\\definepalet" +
      "[${1:name}]" +
      "[${2:alert=green, calm=grey, name}]" +
      "$3"
  "\\definepapersize":
    prefix: "definepapersize"
    body: "\\\\definepapersize" +
      "[${1:name}]" +
      "[${2:.width=${3:name}, height=${4:name}}]" +
      "$5"
  "\\defineparagraphs":
    prefix: "defineparagraphs"
    body: "\\\\defineparagraphs" +
      "[${1:name}]" +
      "[${2:before=${3:name}, after=${4:name}}]" +
      "$5"
  "\\definerawfont":
    prefix: "definerawfont"
    body: "\\\\definerawfont" +
      "[${1:name}]" +
      "${2:[${3:file}]}" +
      "${4:[${5:see \\setupinterlinespace parameters}]}" +
      "$6"
  "\\definereferenceformat":
    prefix: "definereferenceformat"
    body: "\\\\definereferenceformat" +
      "[${1:name}]" +
      "[${2:left=${3:name}, right=${4:name}, text=${5:name}}]" +
      "$6"
  "\\defineregister": # ??
    prefix: "defineregister"
    body: "\\\\defineregister" +
      "[${1:name}]" +
      "[${2:name}]" +
      "$3"
  "\\definesection":
    prefix: "definesection"
    body: "\\\\definesection" +
      "[${1:name}]" +
      "$2"
  "\\definesectionblock":
    prefix: "definesectionblock"
    body: "\\\\definesectionblock" +
      "[${1:name}]" +
      "${2:[${3:see \\setupsectionblock parameters}]}" +
      "$4"
  "\\definesorting": # ??
    prefix: "definesorting"
    body: "\\\\definesorting" +
      "[${1:singlular}]" +
      "[${2:plural}]" +
      "${3:[${4:command}]}" +
      "$5"
  "\\definestartstop":
    prefix: "definestartstop"
    body: "\\\\definestartstop" +
      "[${1:name}]" +
      "[${2:before=${3:name}, after=${4:name}, style=${5:name}$6}]" +
      "$7"
  "\\definesymbol":
    prefix: "definesymbol"
    body: "\\\\definesymbol" +
      "[${1:name}]" +
      "[${2:text}]" +
      "$3"
  "\\definesynonyms":
    comment: "Sort of alias for commands, listed with \\placelistofname"
    prefix: "definesynonyms"
    body: "\\\\definesynonyms" +
      "[${1:singular}]" +
      "[${2:plural}]" +
      "[${3:command}]" +
      "${4:[${5:description text}]}" +
      "$6"
  "\\definetabletemplate":
    prefix: "definetabletemplate"
    body: "\\\\definetabletemplate" +
      "[${1:name}]" +
      "[${2:style of table}]" +
      "$3"
  "\\definetabulate": # ??
    prefix: "definetabulate"
    body: "\\\\definetabulate" +
      "[${1:name}]" +
      "${2:[${3:name of variant or sub-part}]}" +
      "[${4:text}]" +
      "$5"
  "\\definetext": # ??
    prefix: "definetext"
    body: "\\\\definetext" +
      "[${1:name}]" +
      "[${2:header footer}]" +
      "[${3:name}]" +
      "${4:[${5:name}]}" +
      "${6:[${7:name}]}" +
      "$8"
  "\\definetextposition":
    prefix: "definetextposition"
    body: "\\\\definetextposition" +
      "[${1:name}]" +
      "${2:[${3:see \\setuppositioning parameters}]}" +
      "$4"
  "\\definetextvariable": # ??
    prefix: "definetextvariable"
    body: "\\\\definetextvariable" +
      "[${1:see \\setuptextvariable}]" +
      "${2:[${3:name}]}" +
      "${4:[${5:options}]}" +
      "$6"
  "\\definetype":
    prefix: "definetype"
    body: "\\\\definetype" +
      "[${1:name}]" +
      "[${2:see \\setuptype}]" +
      "$3"
  "\\definetyping":
    prefix: "definetyping"
    body: "\\\\definetyping" +
      "[${1:name}]" +
      "[${2:see \\setuptyping}]" +
      "$3"
  "\\defineversion": # ??
    prefix: "defineversion"
    body: "\\\\defineversion" +
      "[${1:name}]" +
      "[${2:number}]" +
      "$3"
  "\\determineheadnumber":
    prefix: "determineheadnumber"
    body: "\\\\determineheadnumber" +
      "[${1:section}]" +
      "$2"
  "\\determinelistcharacteristics":
    prefix: "determinelistcharacteristics"
    body: "\\\\determinelistcharacteristics" +
      "[${1:name}]" +
      "${2:[${3:see \\setuplist}]}" +
      "$4"
  "\\disableinteractionmenu":
    prefix: "disableinteractionmenu"
    body: "\\\\disableinteractionmenu" +
      "[${1:left right top bottom name}]" +
      "[${2:ref, name}]" +
      "$3"
  "\\emptylines":
    prefix: "emptylines"
    body: "\\\\emptylines" +
      "[${1:number of empty lines}]" +
      "$2"
  "\\externalfigure":
    prefix: "externalfigure"
    body: "\\\\externalfigure" +
      "[${1:file}]" +
      "[${2:see \\useexternalfigure}]" +
      "$3"
  "\\field":
    prefix: "field"
    body: "\\\\field" +
      "[${1:name}]" +
      "$2"
  "\\fieldstack":
    prefix: "fieldstack"
    body: "\\\\fieldstack" +
      "[${1:name}]" +
      "${2:[${3:names}]}" +
      "${4:[${5:see \\setupfields}]}" +
      "$6"
  "\\fillinline":
    comment: "A line on which the reader may fill in an answer"
    prefix: "fillinline"
    body: "\\\\fillinline" +
      "${1:[${2:see \\setupfillinlines}]}" +
      "${3:text}" +
      "$4"
  "\\fillinrules":
    prefix: "fillinrules"
    body: "\\\\fillinrules" +
      "${1:[${2:see \\setupfillinrules}]}" +
      "${{3:text before fill in}}" +
      "${{4:text}}" +
      "$5"
  "\\fillintext":
    prefix: "fillintext"
    body: "\\\\fillintext" +
      "${1:[${2:see \\setupfillinrules}]}" +
      "{${3:text}}" +
      "{${4:text}}" +
      "$5"
  "\\fitfield":
    prefix: "fitfield"
    body: "\\\\fitfield" +
      "[${1:name}]" +
      "$2"
  "\\fixedspaces":
    comment: "Force ~ spacing to be equal to 0 glyph"
    prefix: "fixedspaces"
    body: "\\\\fixedspaces" +
      "$1"
  "\\followprofile":
    prefix: "followprofile"
    body: "\\\\followprofile" +
      "{${1:name}}" +
      "[${2:name}]" +
      "$3"
  "\\followprofileversion":
    prefix: "followprofileversion"
    body: "\\\\followprofileversion" +
      "{${1:name}}" +
      "[${2:name}]" +
      "[${3:name}]" +
      "$4"
  "\\followversion":
    prefix: "followversion"
    body: "\\\\followversion" +
      "{${1:name}}" +
      "[${2:name}]" +
      "$3"
  "\\footnote":
    prefix: "footnote"
    body: "\\\\footnote" +
      "${1:[${2:label}]}" +
      "{${3:text}}" +
      "$4"
  "\\fraction":
    prefix: "fraction"
    body: "\\\\fraction" +
      "{${1:numerator}}" +
      "{${2:denominator}}" +
      "$3"
  "\\framed":
    prefix: "framed"
    body: "\\\\framed" +
      "${1:[${2:	see \\setupframed}]}" +
      "{${3:text}}" +
      "$4"
  "\\from":
    prefix: "from"
    body: "\\\\from" +
      "[${1:ref}]" +
      "$2"
  "\\getbuffer":
    prefix: "getbuffer"
    body: "\\\\getbuffer" +
      "${1:text buffer name}" +
      "$3"
  "\\getmarking":
    prefix: "getmarking"
    body: "\\\\getmarking" +
      "[${1:name}]" +
      "[${2:name}]" +
      "$3"
  "\\godown":
    prefix: "godown"
    body: "\\\\godown" +
      "[${1:vertical spaces}]" +
      "$2"
  "\\goto":
    prefix: "goto"
    body: "\\\\goto" +
      "{${1:text}}" +
      "{${2:label}}" +
      "$3"
  "\\gotobox":
    prefix: "gotobox"
    body: "\\\\gotobox" +
      "{${1:text}}" +
      "{${2:label}}" +
      "$3"
  "\\graycolor":
    prefix: "graycolor"
    body: "\\\\graycolor" +
      "[${1:text}]" +
      "$2"
  "\\grid":
    prefix: "grid"
    body: "\\\\grid" +
      "[${1:grid parameters}]" +
      "$2"
  "\\hairline":
    prefix: "hairline"
    body: "\\\\hairline" +
      "$1"
  "\\head":
    comment: "Head of itemize"
    prefix: "head"
    body: "\\\\head" +
      "${1:[${2:ref, name}]}" +
      "$3"
  "\\headnumber":
    prefix: "headnumber"
    body: "\\\\headnumber" +
      "${1:[${2:section}]}" +
      "$3"
  "\\headtext":
    prefix: "headtext"
    body: "\\\\headtext" +
      "{${1:text}}" +
      "$2"
  "\\hideblocks":
    comment: "From here on, do not print blocks where they are defined"
    prefix: "hideblocks"
    body: "\\\\hideblocks" +
      "[${1:block types to hide}]" +
      "${2:[${3:tags to further specify what to hide}]}" +
      "$4"
  "\\high":
    prefix: "high"
    body: "\\\\high" +
      "{${1:text}}" +
      "$2"
  "\\hl":
    prefix: "hl"
    body: "\\\\hl" +
      "[${1:width}]" +
      "$2"
  "\\in":
    prefix: "in"
    body: "\\\\in" +
      "{${1:text before}}" +
      "{${2:text after}}" +
      "[${3:ref}]" +
      "$4"
  "\\inframed":
    comment: "It ensures that the baseline of the framed text aligns with the baseline of the surrounding text. In all other respects, it is identical to \\framed."
    prefix: "inframed"
    body: "\\\\inframed" +
      "${1:[${2:see \\setupframed}]}" +
      "{${3:text}}" +
      "$4"
  "\\ininner":
    prefix: "ininner"
    body: "\\\\ininner" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\inleft":
    prefix: "inleft"
    body: "\\\\inleft" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\inline":
    prefix: "inline"
    body: "\\\\inline" +
      "${1:[${2:text prefix}]}" +
      "[${3:ref}]" +
      "$4"
  "\\inmargin":
    prefix: "inmargin"
    body: "\\\\inmargin" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\inouter":
    prefix: "inouter"
    body: "\\\\inouter" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\inright":
    prefix: "inright"
    body: "\\\\inright" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\installlanguage":
    prefix: "installlanguage"
    body: "\\\\installlanguage" +
      "[${1:name}]" +
      "[${2:language parameters}]" +
      "$3"
  "\\item":
    prefix: "item"
    body: "\\\\item" +
      "${1:[${2:ref, name}]}" +
      "$3"
  "\\items":
    prefix: "items"
    body: "\\\\items" +
      "${1:[${2:see \\setupitems}]}" +
      "{${3:item, item}}" +
      "$4"
  "\\keepblocks":
    comment: "From here on typeset blocks where they are defined, as well as when they are recalled"
    prefix: "keepblocks"
    body: "\\\\keepblocks" +
      "[${1:name of blocks}]" +
      "$4"
  "\\labeltext":
    comment: "Retrieve a label text defined earlier with \\setuplabeltext"
    prefix: "labeltext"
    body: "\\\\labeltext" +
      "{${1:label name}}" +
      "$2"
  "\\language":
    prefix: "language"
    body: "\\\\language" +
      "[${1:language label}]" +
      "$2"
  "\\leftaligned":
    prefix: "leftaligned"
    body: "\\\\leftaligned" +
      "{${1:text}}" +
      "$2"
  "\\listsymbol":
    prefix: "listsymbol"
    body: "\\\\listsymbol" +
      "[${1:name}]" +
      "{${2:text}}" +
      "$3"
  "\\low":
    comment: "Text to put lower that normal"
    prefix: "low"
    body: "\\\\low" +
      "{${1:text}}" +
      "$2"
  "\\mainlanguage":
    prefix: "mainlanguage"
    body: "\\\\mainlanguage" +
      "[${1:language label}]" +
      "$2"
  "\\mar":
    comment: "Used in itemizations. Prints an item with a remark in the margin"
    prefix: "mar"
    body: "\\\\mar" +
      "{${3:text}}" +
      "$4"
  "\\margintext":
    prefix: "margintext"
    body: "\\\\margintext" +
      "${1:[${2:+ - low}]}" +
      "${3:[${4:ref}]}" +
      "{${5:text}}" +
      "$6"
  "\\marking":
    prefix: "marking"
    body: "\\\\marking" +
      "[${1:name}]" +
      "{${2:name}}" +
      "$3"
  "\\mathematics":
    prefix: "mathematics"
    body: "\\\\mathematics" +
      "{${1:formula}}" +
      "$2"
  "\\mediaeval":
    prefix: "mediaeval"
    body: "\\\\mediaeval" +
      "{${1:text to old style}}" +
      "$2"
  "\\midaligned":
    prefix: "midaligned"
    body: "\\\\midaligned" +
      "{${1:text}}" +
      "$2"
  "\\mirror":
    prefix: "mirror"
    body: "\\\\mirror" +
      "{${1:text to mirror}}" +
      "$2"
  "\\month":
    prefix: "month"
    body: "\\\\month" +
      "{${1:month number}}" +
      "$2"
  "\\noheaderandfooterlines":
    comment: "The command disables the header/footer on the page where \noheaderandfooterlines was inserted"
    prefix: "noheaderandfooterlines"
    body: "\\\\noheaderandfooterlines" +
      "$1"
  "\\nolist":
    prefix: "nolist"
    body: "\\\\nolist" +
      "{${1:text}}" +
      "$2"
  "\\nomarking":
    prefix: "nomarking"
    body: "\\\\nomarking" +
      "{${1:text}}" +
      "$2"
  "\\nomoreblocks":
    prefix: "nomoreblocks"
    body: "\\\\nomoreblocks" +
      "$1"
  "\\nop":
    comment: "Used in itemizations. Prints an item without a mark, and without incrementing the item counter"
    prefix: "nop"
    body: "\\\\nop" +
      "$1"
  "\\note":
    prefix: "note"
    body: "\\\\note" +
      "[${1:ref}]" +
      "$2"
  "\\nowhitespace":
    prefix: "nowhitespace"
    body: "\\\\nowhitespace" +
      "$1"
  "\\numbers":
    prefix: "numbers"
    body: "\\\\numbers" +
      "{${1:name}}" +
      "$2"
  "\\overbar":
    prefix: "overbar"
    body: "\\\\overbar" +
      "{${1:text}}" +
      "$2"
  "\\overbars":
